    "repository": {
      "type": "git",
      "url": "https://github.com/maxild/Prelude/"
    },
    "releaseNotes": ".NET Core 1.0 and .NET Framework 4.5.2 support",
    "copyright": "Copyright © Morten Maxild 2016",
    "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
    "projectUrl": "https://github.com/maxild/Prelude/",
    "owners": [
      "Morten Maxild"
    ],
    "tags": [
      "prelude"
    ]
  },
  "dependencies": {
    "NETStandard.Library": "1.6.0"
  },
  "frameworks": {
    "net452": {},
    "netstandard1.5": {}
  }
}